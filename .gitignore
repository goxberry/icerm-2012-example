.Spotlight-V100
.Trashes

# Ignore Mercurial files
/.hg/*
*/.hg/*
.hgignore

# Ignore CVS files
/CVS/*
*/CVS/*
.cvsignore
*/.cvsignore

# Ignore SVN files
.svn/

*.py[cod]

